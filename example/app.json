{
  "expo": {
    "name": "expo-finance-kit-example",
    "slug": "expo-finance-kit-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "expo.modules.financekit.example",
      "infoPlist": {
        "NSFinancialDataDescription": "This app needs access to your financial data to display transaction history and account information."
      },
      "entitlements": {
        "com.apple.developer.financekit": ["financial-data"],
        "com.apple.developer.financekit.transaction-picker": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "expo.modules.financekit.example"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}